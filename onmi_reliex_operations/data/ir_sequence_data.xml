<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for worksheet -->
        <record id="seq_worksheet" model="ir.sequence">
            <field name="name">Worksheet</field>
            <field name="code">worksheet.part</field>
            <field name="prefix">PT/</field>
            <field name="padding">5</field>
        </record>

        <!-- Sequences for work.order.clean -->
        <record id="seq_work_order_clean" model="ir.sequence">
            <field name="name">Work Order Cleaning</field>
            <field name="code">work.order.clean</field>
            <field name="prefix">OTL/</field>
            <field name="padding">5</field>
        </record>

        <!-- Sequences for work.order.plant -->
        <record id="seq_work_order_plant" model="ir.sequence">
            <field name="name">Work Order Plant</field>
            <field name="code">work.order.plant</field>
            <field name="prefix">OTI/</field>
            <field name="padding">5</field>
        </record>

        <!-- Sequences for incident -->
        <record id="seq_incident" model="ir.sequence">
            <field name="name">Incident</field>
            <field name="code">incident</field>
            <field name="prefix">I/</field>
            <field name="padding">5</field>
        </record>

        <!-- Sequences for checklist -->
        <record id="seq_checklist" model="ir.sequence">
            <field name="name">Checklist</field>
            <field name="code">checklist</field>
            <field name="prefix">CHECK/</field>
            <field name="padding">5</field>
        </record>

        <!-- Sequences for material lists -->
        <record id="seq_material_list" model="ir.sequence">
            <field name="name">seq.material.list</field>
            <field name="code">material.list</field>
            <field name="prefix">LM/</field>
            <field name="padding">5</field>
        </record>

    </data>
</odoo>
